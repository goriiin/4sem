{{define "table"}}
    <table class="table table-striped table-bordered">
        <thead>
        <tr>
            {{range $index, $header := .Fields}}
                <th>{{$header}}</th>
            {{end}}
        </tr>
        </thead>
    </table>
    <table class="table table-striped table-bordered">
        <tbody>
        {{range $index := .Data}}
            <tr>
                <td>{{$index}}</td>
            </tr>
        {{end}}
        </tbody>
    </table>
{{end}}